<% ## SHOW BY DATE RATHER THAN INDIVIDUAL BIRDS %>
<h1>Birds Banded on <%= @session[:date_string]%></h1>
<table>
<tr><th>Alpha Code</th><th>Species</th><th>Number Banded</th></tr>
<%@count_by_species.each do |key,value| %>
<tr><td><%=key.code%></td><td><%=key.name%></td><td><%=value%></td>
</tr>
<%end%>
</table>
<form action="/birds/new">
    <input type="submit" value="Add more birds" />
</form>
<form action="/birds/:date/add_narrative">
    <input type="submit" value="Add narrative" />
</form>
<form action="/birds/:date/report">
    <input type="submit" value="Generate report" />
</form>
